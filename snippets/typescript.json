{
  "Print to console": {
    "prefix": "log",
    "body": ["console.log('$1');", "$2"],
    "description": "Log output to console"
  },
  "JSDoc typescript": {
    "prefix": "doc",
    "body": [
      "",
      "/**",
      "* $1",
      "* @param $2 $3",
      "* @returns ${4:string}",
      "*/"
    ],
    "description": "Expand JSDoc snippet"
  },
  "Use State": {
    "prefix": "uses",
    "body": ["const [$1, ${2: set}] = React.useState($3);"]
  },
  "Use Effect": {
    "prefix": "usee",
    "body": ["React.useEffect( () => {", "  $1", "}, [$2]);"]
  },
  "Use Ref": {
    "prefix": "user",
    "body": ["const $1Ref = React.useRef($2);"]
  },
  "Use Memo": {
    "prefix": "usem",
    "body": ["const $1 = React.useMemo( () => $2, [$3]);"]
  },
  "Use Callback": {
    "prefix": "usec",
    "body": ["const $1 = React.useCallback( () => {", "  $2", "}, [$3]);"]
  },
  "Backqoute": {
    "prefix": "backqoute",
    "body": ["`$1`"]
  }
}
